<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kalvin.mydroidcafev1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_my_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_my_app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyDroidCafeV1">
        <activity android:name=".DonutActivity"></activity>
        <activity android:name=".FroyoActivity"></activity>
        <activity android:name=".SandwichActivity"></activity>
        <activity android:name=".RatingActivity"></activity>
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>